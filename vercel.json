{
	"version": 2,
	"routes": [
		{
			"src": "/(en|sv)/(.*)",
			"headers": {
				"Cache-Control": "public, max-age=86400, must-revalidate"
			}
		}
	],
	"images": {
		"sizes": [320, 480, 640, 750, 828, 1080, 1200, 1366, 1440, 1920, 2048, 2560, 3840],
		"minimumCacheTTL": 86400,
		"formats": ["image/webp"],
		"contentSecurityPolicy": "script-src 'none'; frame-src 'none'; sandbox;",
		"contentDispositionType": "inline",
		"domains": ["localhost", "api-gottsunda.upwego.se", "api-dev-gottsunda.upwego.se"]
	}
}
